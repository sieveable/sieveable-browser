{
  "parser": "babel-eslint",
    "rules": {
      "strict": 0,
        "indent": [
            1,
            2
        ],
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "globals":{
      "__dirname": false,
      "require": false
    },
    "plugins": [
        "react"
    ]
}
